#!/usr/bin/python3.5
from tensorflow.examples.tutorials.mnist import input_data
mnist = input_data.read_data_sets("MNIST_data/", one_hot=True)
import tensorflow as tf
x=tf.placeholder(tf.float32, [None,784])
#autoencoder layer 1
w11=tf.Variable(tf.zeros([784,392]))
w12=tf.Variable(tf.zeros([392,784]))
b1=tf.Variable(tf.zeros([784]))
#layer 2
w21=tf.Variable(tf.zeros([392,196]))
w22=tf.Variable(tf.zeros([196,392]))
b2=tf.Variable(tf.zeros([392]))
#layer 3
w31=tf.Variable(tf.zeros([196,10]))
b3=tf.Variable(tf.zeros([10]))

